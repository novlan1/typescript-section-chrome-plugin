{
  "manifest_version": 2,
  "name": "Typescript官网章节",
  "description": "Typescript官网章节",
  "version": "1.0.0",
  "homepage_url": "https://uwayfly.com",
  "icons": {
    "128": "img/icon.png",
    "16": "img/icon.png",
    "48": "img/icon.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Cookie Injector"
  },
  "content_security_policy": "style-src 'self' 'unsafe-inline';script-src 'self' 'unsafe-eval' https://cdn.bootcss.com; object-src 'self' ;",
  "content_scripts": [
    {
      "js": [
        "js/content-script.js"
      ],
      "matches": [
        "https://www.tslang.cn/docs/handbook/*"
      ]
    }
  ],
  "permissions": [
    "contextMenus"
  ]
}